{
    "name": "xhamster",
    "module": "index.ts",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "build": "rimraf dist && tsc",
        "start": "npm run build && node dist/index.js",
        "dev": "nodemon src/index.ts",
        "format": "prettier --write  \"src/**/*.{ts,tsx,js,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json,md}\"",
        "prepare": "husky",
        "lint": "eslint",
        "prod": "pm2 start index.ts",
        "stop": "pm2 stop all",
        "docker:run": "docker-compose up --build",
        "docker:stop": "docker-compose down",
        "docker:logs": "docker-compose logs"
    },
    "keywords": [
        "docker",
        "bun",
        "typescript",
        "express.js",
        "redis"
    ],
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@types/bun": "latest",
        "@types/express": "^5.0.3",
        "@types/node": "^24.2.0",
        "eslint": "^9.32.0",
        "eslint-plugin-prettier": "^5.5.3",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-unicorn": "^60.0.0",
        "globals": "^16.3.0",
        "husky": "^9.1.7",
        "ioredis": "^5.7.0",
        "jest": "^30.0.5",
        "jiti": "^2.5.1",
        "prettier": "^3.6.2",
        "supertest": "^7.1.4",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0",
        "zod": "^4.0.14"
    },
    "dependencies": {
        "@types/axios": "^0.14.4",
        "@types/cheerio": "^1.0.0",
        "@types/morgan": "^1.9.10",
        "@types/redis": "^4.0.11",
        "@types/supertest": "^6.0.3",
        "axios": "^1.11.0",
        "axios-retry": "^4.5.0",
        "cheerio": "^1.1.2",
        "dotenv": "^17.2.1",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "impit": "^0.5.4",
        "morgan": "^1.10.1",
        "pm2": "^6.0.8",
        "redis": "^5.8.0",
        "rimraf": "^6.0.1",
        "winston": "^3.17.0"
    }
}
